{
  "name": "cdk-starter",
  "version": "0.1.0",
  "bin": {
    "cdk-starter": "bin/cdk-starter.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix",
    "cdk-deploy": "npx cdk deploy --profile hahuaz-root",
    "cdk-list": "npx cdk ls",
    "cdk-watch": "npx cdk watch PROD-hahuaz --hotswap-fallback --logs --profile hahuaz-root"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.41.0",
    "constructs": "^10.1.98",
    "dotenv": "^16.3.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.93",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.14.36",
    "eslint": "^8.23.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
